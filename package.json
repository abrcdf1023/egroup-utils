{
  "name": "@e-group/frontend-utils",
  "version": "2.3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eGroupTeam/frontend-utils.git"
  },
  "author": "JerryP <egroup.jerrylee@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/index.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/index.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/index.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/index.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run clean && npm run lint && npm run build"
  },
  "devDependencies": {
    "@types/immutable": "^3.8.7",
    "@types/redux-actions": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-uglify": "^3.0.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.9",
    "redux-actions": "^2.6.1",
    "rxjs": "^6.2.2"
  },
  "description": "eGroup team's frontend utils",
  "bugs": {
    "url": "https://github.com/eGroupTeam/frontend-utils/issues"
  },
  "homepage": "https://github.com/eGroupTeam/frontend-utils#readme"
}
